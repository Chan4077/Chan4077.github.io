---
layout: feed
---

<?xml version="1.0" encoding="utf-8"?>
<feed 
	xmlns="http://www.w3.org/2005/Atom">
	<title>Edric Chan's Blog</title>
	<link href="http://chan4077.github.io/blog/atom.xml" rel="self"/>
	<link href="http://chan4077.github.io/blog"/>
	<updated>{{ site.time | date_to_xmlschema }}</updated>
	<id>http://chan4077.github.io/blog</id>
	<author>
		<name>Edric Chan</name>
		<email>edric.chan.1997@gmail.com</email>
	</author>
 {% for post in site.posts %} 
	<entry>
		<title>{{ post.title }}</title>
		<link href="https://chan4077.github.io/{{post.url}}"/>
		<updated>{{ post.date | date_to_xmlschema }}</updated>
		<id>https://chan4077.github.io{{ post.id }}</id>
		<content type="html">{{ post.content | xml_escape }}</content>
	</entry> {% endfor %} 
</feed>